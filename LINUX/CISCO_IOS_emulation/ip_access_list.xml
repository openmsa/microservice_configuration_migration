  <command name="IMPORT">
    <operation><![CDATA[cat /config/cisco_ios/test.conf]]></operation>
    <post_template>{$seq_number=10}
{$mask_length=&quot;&quot;}
{if isset($params.access_list)}
	{foreach $params.access_list as $acl}
	    {if isset($acl.acl_src_imask)}
	    	{* convert inverted mask to mask length for compatibility 
	    	with IOS XR ipv4 acccess-list *}
			{capture}
			{convert_imask_to_length input={$acl.acl_src_imask} var=&quot;mask_length&quot;}
			{/capture}
			{assign_object_variable 
				var=&quot;access_list.{$acl@index}.acl_src_mask_length&quot; 
				value=$mask_length}
	    {/if}
	    {if isset($acl.acl_dest_imask)}
	    	{* convert inverted mask to mask length for compatibility 
	    	with IOS XR ipv4 acccess-list *}
			{capture}
			{convert_imask_to_length input={$acl.acl_dest_imask} var=&quot;mask_length&quot;}
			{/capture}
			{assign_object_variable 
				var=&quot;access_list.{$acl@index}.acl_dest_mask_length&quot; 
				value=$mask_length}
	    {/if}
	    {if isset($acl.acl_protocol) &amp;&amp; $acl.acl_protocol == &quot;ip&quot;}
	    	{assign_object_variable 
	    		var=&quot;access_list.{$acl@index}.acl_protocol&quot; 
	    		value=&quot;ipv4&quot;}
		{/if}
		{* allocate a sequence number for compatibility with IOS XR *}
	    {assign_object_variable 
	    	var=&quot;access_list.{$acl@index}.acl_seq_number&quot;
	    	value=$seq_number}
	    {$seq_number=$seq_number+10}
	{/foreach}
{/if}
{* fetch the data from the access-list and convert to arrays to match 
   the ip access-list format in both IOS and IOS XR *}
{if isset($access_list)}
	{$idx=0}
	{$seq_number=10}
	{* {foreach $access_list|@sortby_typed:&quot;acl_src_ip:ip,acl_src:string&quot; as $acl} *}
	 {foreach $access_list as $acl}
		{if ($acl.acl_id == $params.object_id) &amp;&amp; (!isset($acl.acl_src) || $acl.acl_src==&quot;&quot;)}
			{$keys = array_keys($acl)}
			{foreach $keys as $key}
			{assign_object_variable 
				var=&quot;access_list.{$idx}.{$key}&quot; 
				value={$acl.$key}}
			{/foreach}	
			{* allocate a sequence number for compatibility with IOS XR *}
			{assign_object_variable 
				var=&quot;access_list.{$idx}.acl_seq_number&quot; 
				value={$seq_number}}			
			{$idx=$idx+1}
		    {$seq_number=$seq_number+10}
		{/if}
	{/foreach}
	{* {foreach $access_list|@sortby_typed:&quot;acl_src_ip:string&quot; as $acl} *}
	 {foreach $access_list as $acl}
		{if ($acl.acl_id == $params.object_id) &amp;&amp; isset($acl.acl_src) &amp;&amp; $acl.acl_src==&quot;any&quot;}
			{$keys = array_keys($acl)}
			{foreach $keys as $key}
			{assign_object_variable 
				var=&quot;access_list.{$idx}.{$key}&quot; 
				value={$acl.$key}}
			{/foreach}	
			{* allocate a sequence number for compatibility with IOS XR *}
			{assign_object_variable 
				var=&quot;access_list.{$idx}.acl_seq_number&quot; 
				value={$seq_number}}			
			{$idx=$idx+1}
		    {$seq_number=$seq_number+10}
		{/if}
	{/foreach}
{/if}
</post_template>
    <parser>
      <section>
        <regexp>@^\s*(?&lt;type&gt;ip|ipv6)\s+access-list\s+(extended|standard)?\s*(?&lt;object_id&gt;\S+)@</regexp>
        <regexp>@^\s*access-list\s+(?&lt;object_id&gt;\d+)@</regexp>
      </section>
      <lines>
        <line>
          <array name="access_list">
            <regexp>@^\s*(?&lt;acl_rule&gt;permit|deny)\s*(?&lt;acl_protocol&gt;ip|udp|tcp|pim|ospf|\s\d+\s)?\s*(?&lt;acl_src&gt;any)\s*(\s*(?&lt;acl_src_port_op&gt;eq|lt|gt|range)\s+(?&lt;acl_src_port&gt;\S+|\d+(\s\d+)?))?\s*(?&lt;acl_dest&gt;any|host)?\s*(?&lt;acl_dest_ip&gt;([0-9]{1,3}\.){3}[0-9]{1,3})?\s*(?&lt;acl_dest_imask&gt;([0-9]{1,3}\.){3}[0-9]{1,3})?\s*((\s*(?&lt;acl_dest_port_op&gt;eq|lt|gt|range)\s+(?&lt;acl_dest_port&gt;\S+|\d+(\s\d+)?))|(\s*(?&lt;acl_log&gt;log|log-input))|(\s*(?&lt;acl_fragments&gt;fragments))|(\s*dscp\s+(?&lt;acl_dscp&gt;\S+)))*$@</regexp>
            <regexp>@^\s*(?&lt;acl_rule&gt;permit|deny)\s*(?&lt;acl_protocol&gt;ip|udp|tcp|pim|ospf|\s\d+\s)?\s*(?&lt;acl_src&gt;host)?\s*(?&lt;acl_src_ip&gt;([0-9]{1,3}\.){3}[0-9]{1,3})?\s*(?&lt;acl_src_imask&gt;([0-9]{1,3}\.){3}[0-9]{1,3})?\s*((?&lt;acl_src_port_op&gt;eq|lt|gt|range)\s+(?&lt;acl_src_port&gt;\S+|\d+(\s\d+)?))?\s*(?&lt;acl_dest&gt;(any|host))?\s*(?&lt;acl_dest_ip&gt;([0-9]{1,3}\.){3}[0-9]{1,3})?\s*(?&lt;acl_dest_imask&gt;([0-9]{1,3}\.){3}[0-9]{1,3})?\s*(((\s*(?&lt;acl_dest_port_op&gt;eq|lt|gt|range)\s+(?&lt;acl_dest_port&gt;\S+|\d+(\s\d+)?))|(\s*(?&lt;acl_log&gt;log|log-input))|(\s*(?&lt;acl_fragments&gt;fragments))|(\s*dscp\s+(?&lt;acl_dscp&gt;\S+))))*$@</regexp>
